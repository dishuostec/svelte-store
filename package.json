{
	"name": "@dishuostec/svelte-store",
	"version": "1.0.0",
	"description": "Touchable store",
	"keywords": [
		"svelte store",
		"deep equal",
		"touchable store",
		"stopwatch store",
		"countdown store",
		"derived record store"
	],
	"access": "public",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dishuostec/svelte-store.git"
	},
	"author": "dishuostec",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dishuostec/svelte-store/issues"
	},
	"homepage": "https://github.com/dishuostec/svelte-store#readme",
	"publishConfig": {
		"directory": "dist"
	},
	"scripts": {
		"build": "node scripts/build.js",
		"tsd": "tsc --emitDeclarationOnly",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test": "ts-mocha -p tsconfig.test.json src/**/*.spec.ts",
		"check": "tsc --noEmit",
		"release": "pnpm test && pnpm build && pnpm tsd && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && pnpm publish --access public"
	},
	"devDependencies": {
		"@types/mocha": "^9.1.0",
		"@types/node": "^17.0.18",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"esbuild": "^0.14.21",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"mocha": "^9.1.3",
		"prettier": "^2.5.1",
		"sucrase": "^3.20.3",
		"svelte": "^3.46.4",
		"ts-mocha": "^9.0.2",
		"typescript": "^4.5.5"
	},
	"type": "module",
	"dependencies": {
		"fast-deep-equal": "^3.1.3"
	}
}
